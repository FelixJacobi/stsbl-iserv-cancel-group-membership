Test "resolve conflict with iserv3-grouprequest"
  dpkg-query --showformat='${Status}\n' --show \
    'iserv3-grouprequest' 2> /dev/null | \
    grep 'install ok installed' &&
  dpkg-query --showformat='${Status}\n' --show \
    'iserv-grouprequest' 2> /dev/null | \
    grep 'install ok installed' &&  
  ---
  /usr/lib/iserv/aptitude_auto -o Dpkg::Options::="--force-overwrite" install \
     stsbl-iserv-cancel-group-membership iserv-grouprequest iserv3-grouprequest

